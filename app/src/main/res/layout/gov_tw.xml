    android:layout_height="match_parent">
<TableLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="0,1">
    <TableRow>
        <TextView
            android:text="@string/tmp_str"/>
        <TextView
            android:text="@string/tmp_str"
            android:id="@+id/current_gov"/>
    </TableRow>
    <TableRow>
